{
  "tooltip": {
    "trigger": "axis"
  },
  "grid": {
    "left": "3%",
    "right": "4%",
    "bottom": "10%",
    "containLabel": true
  },
  "xAxis": {
    "show": true
  },
  "yAxis": {
    "show": true
  },
  "series": [{
      "lineStyle": {
          "color": "{{ theme.palette.colors.primary }}",
          "width": 2
      },
      "itemStyle": {
          "color": "{{ theme.palette.colors.primary_dark }}"
      }
  }]
}
